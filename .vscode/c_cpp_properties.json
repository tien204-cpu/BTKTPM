{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [
            "-Wall",
            "-Wextra",
            "-g3",
            "${file}",
            "-o",
            "${fileDirname}/output/${fileBasenameNoExtension}",
            "-lm"
      ]
    }
  ],
  "version": 4
}