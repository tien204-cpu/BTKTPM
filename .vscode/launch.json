{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [    
        "-Wall",
        "-Wextra",
        "-g3",
        "${file}",
        "-o",
        "${fileDirname}/output/${fileBasenameNoExtension}",
        "-lm"
      ],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/workspaces/BTKTPM/Lab1/Bai2a",
      "program": "/workspaces/BTKTPM/Lab1/Bai2a/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}